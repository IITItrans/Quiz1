<p id="notice"><%= notice %></p>

<h1>Listing Student Mcqs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_mcqs.each do |student_mcq| %>
      <tr>
        <td><%= link_to 'Show', student_mcq %></td>
        <td><%= link_to 'Edit', edit_student_mcq_path(student_mcq) %></td>
        <td><%= link_to 'Destroy', student_mcq, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student mcq', new_student_mcq_path %>
