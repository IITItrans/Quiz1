<p id="notice"><%= notice %></p>

<h1>Listing Student Quizzes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_quizzes.each do |student_quiz| %>
      <tr>
        <td><%= link_to 'Show', student_quiz %></td>
        <td><%= link_to 'Edit', edit_student_quiz_path(student_quiz) %></td>
        <td><%= link_to 'Destroy', student_quiz, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student quiz', new_student_quiz_path %>
